{
    "version": 2,
    "builds": [
        { 
            "src": "server.js", 
            "use": "@vercel/node" 
          } 
        ], 
        "routes": [ 
          { 
            "src": "/(.*)", 
            "dest": "/server.js" 
          } 
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/"
      }
    ],
    "env": {
      "GOOGLE_APPLICATION_CREDENTIALS": {
        "type": "secret",
        "value": "GOOGLE_APPLICATION_CREDENTIALS"
      }
    }
  }