{
    "version": 2,
    "builds": [
        { 
            "src": "server.js", 
            "use": "@vercel/node" 
          } 
        ],
    "routes": [ 
          { 
            "src": "/(.*)", 
            "dest": "/server.js" 
          } 
    ],
    "env": {
      "GOOGLE_APPLICATION_CREDENTIALS": "./google-credentials.json"
    }
  }